{
  "name": "@jawilx/gen-api",
  "type": "module",
  "version": "2.0.0",
  "packageManager": "pnpm@8.12.1",
  "description": "A modern cli tool that auto generate typescript api code from swagger 2 and openapi 3",
  "author": "JawilX <xjh53047@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/JawilX",
  "homepage": "https://github.com/JawilX/gen-api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JawilX/gen-api.git"
  },
  "bugs": "https://github.com/JawilX/gen-api/issues",
  "keywords": [
    "api",
    "typescript",
    "generator",
    "swagger",
    "openapi"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "gen-api": "bin/gen-api.mjs"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/cli.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.7",
    "axios": "^1.6.2",
    "execa": "^8.0.1",
    "picocolors": "^1.0.0",
    "pinyin-pro": "^3.18.5",
    "unconfig": "^0.3.11",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.0",
    "@antfu/ni": "^0.21.12",
    "@types/node": "^20.10.5",
    "@types/yargs": "^17.0.32",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.12.1",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
